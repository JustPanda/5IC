<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javafxapplication2.LoginFXMLController">
   <children>
      <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="393.0" prefWidth="640.0" VBox.vgrow="ALWAYS">
         <children>
            <Label fx:id="nomeChat" layoutX="208.0" layoutY="15.0" prefHeight="27.0" prefWidth="172.0" text="quello che dovrebbe aprire realmente" />
            <Button fx:id="invia" layoutX="586.0" layoutY="354.0" mnemonicParsing="false" onAction="#inviaMessaggio" prefHeight="46.0" prefWidth="52.0" text="Invia" />
            <ScrollPane fx:id="scrollpane" layoutX="199.0" layoutY="43.0" prefHeight="313.0" prefWidth="438.0">
               <content>
                  <VBox fx:id="vbox" prefHeight="310.0" prefWidth="436.0" />
               </content>
            </ScrollPane>
            <TextArea fx:id="testo" layoutX="199.0" layoutY="354.0" prefHeight="46.0" prefWidth="386.0" promptText="Inserisci il messaggio qui.." wrapText="true" />
            <ListView fx:id="listaNomi" onMouseClicked="#onmouseclicked" prefHeight="400.0" prefWidth="200.0" />
            <Button fx:id="reload" layoutX="569.0" layoutY="3.0" mnemonicParsing="false" onAction="#reload" prefHeight="37.0" prefWidth="57.0" text="Reload" />
         </children>
      </AnchorPane>
   </children>
</VBox>
